{
  "expo": {
    "name": "Swiftly",
    "slug": "swifty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Swiftly.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "53.0.0",
    "splash": {
      "image": "./assets/Swiftly.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swifty.app",
      "deploymentTarget": "13.0",
      "config": {
        "googleMapsApiKey": "AIzaSyDUMMY_KEY_FOR_TESTING"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location for in-app navigation and accurate routing.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location for in-app navigation and accurate routing.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location to provide turn-by-turn navigation and real-time delivery updates.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Swiftly.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.swifty.app",
      "minSdkVersion": 23,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDUMMY_KEY_FOR_TESTING"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/Swiftly.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "./expo-navigation-plugin.js"
    ]
  }
}
